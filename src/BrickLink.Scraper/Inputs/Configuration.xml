<Configuration>
  <InventoryFile>Inventory.xml</InventoryFile>
  <CacheTimeHoursPolicy>24</CacheTimeHoursPolicy>
  <HtmlLoadTimeMs>600</HtmlLoadTimeMs>
  <!--Accepted Values are 1 = LowestPrice, 4 = Highest Quantity-->
  <SortPartsBy>1</SortPartsBy>
  <ResultsPerPage>100</ResultsPerPage>
  <MinQuantityPercent>10</MinQuantityPercent>
  <NumberOfSellers>40</NumberOfSellers>
  <HeadlessBrowser>true</HeadlessBrowser>
  <PreferredSellers>
    <Seller>Brock's Bricks</Seller>
    <Seller>StudBee</Seller>
  </PreferredSellers>
  <IgnoredSellers>
    <Seller>BBB Brick Store</Seller>
    <Seller>Bricktropolis</Seller>
    <Seller>Music City Bricks</Seller>
    <Seller>Utica Brix</Seller>
  </IgnoredSellers>
  <SellerWeights>
  	<PricePoint>1.2</PricePoint>
  	<Quantity>1.5</Quantity>
    <Rarity>1.9</Rarity>
  </SellerWeights>
</Configuration>